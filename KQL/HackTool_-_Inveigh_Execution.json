{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "String"
    }
  },
  "resources": [
    {
      "apiVersion": "2023-02-01-preview",
      "id": "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/alertRules/b99a1518-1ad5-4f65-bc95-1ffff97a8fd0')]",
      "kind": "Scheduled",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/b99a1518-1ad5-4f65-bc95-1ffff97a8fd0')]",
      "properties": {
        "alertRuleTemplateName": "b99a1518-1ad5-4f65-bc95-1ffff97a8fd0",
        "author": "Nasreddine Bencherchali (Nextron Systems)",
        "customDetails": null,
        "date": "2022/10/24",
        "description": "Detects the use of Inveigh a cross-platform .NET IPv4/IPv6 machine-in-the-middle tool",
        "displayName": "HackTool - Inveigh Execution",
        "enabled": true,
        "entityMappings": null,
        "eventGroupingSettings": {
          "aggregationKind": "SingleAlert"
        },
        "logsource": {
          "category": "process_creation",
          "product": "windows"
        },
        "modified": "2023/02/04",
        "query": "DeviceProcessEvents\n| where FolderPath endswith \"\\\\Inveigh.exe\" or (ProcessVersionInfoOriginalFileName in~ (\"\\\\Inveigh.exe\", \"\\\\Inveigh.dll\")) or ProcessVersionInfoFileDescription =~ \"Inveigh\" or (ProcessCommandLine contains \" -SpooferIP\" or ProcessCommandLine contains \" -ReplyToIPs \" or ProcessCommandLine contains \" -ReplyToDomains \" or ProcessCommandLine contains \" -ReplyToMACs \" or ProcessCommandLine contains \" -SnifferIP\")",
        "queryFrequency": "P1D",
        "queryPeriod": "P1D",
        "sentinelEntitiesMappings": null,
        "severity": "critical",
        "status": "test",
        "suppressionDuration": "PT5H",
        "suppressionEnabled": false,
        "tactics": null,
        "techniques": [
          "t1003"
        ],
        "templateVersion": "1.0.0",
        "triggerThreshold": 0
      },
      "type": "Microsoft.OperationalInsights/workspaces/providers/alertRules"
    }
  ]
}
