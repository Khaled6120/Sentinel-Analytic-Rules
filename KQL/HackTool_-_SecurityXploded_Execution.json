{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "String"
    }
  },
  "resources": [
    {
      "apiVersion": "2023-02-01-preview",
      "id": "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/alertRules/7679d464-4f74-45e2-9e01-ac66c5eb041a')]",
      "kind": "Scheduled",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/7679d464-4f74-45e2-9e01-ac66c5eb041a')]",
      "properties": {
        "alertRuleTemplateName": "7679d464-4f74-45e2-9e01-ac66c5eb041a",
        "author": "Florian Roth (Nextron Systems)",
        "customDetails": null,
        "date": "2018/12/19",
        "description": "Detects the execution of SecurityXploded Tools",
        "displayName": "HackTool - SecurityXploded Execution",
        "enabled": true,
        "entityMappings": null,
        "eventGroupingSettings": {
          "aggregationKind": "SingleAlert"
        },
        "logsource": {
          "category": "process_creation",
          "product": "windows"
        },
        "modified": "2023/02/04",
        "query": "DeviceProcessEvents\n| where ProcessVersionInfoCompanyName =~ \"SecurityXploded\" or FolderPath endswith \"PasswordDump.exe\" or ProcessVersionInfoOriginalFileName endswith \"PasswordDump.exe\"",
        "queryFrequency": "P1D",
        "queryPeriod": "P1D",
        "sentinelEntitiesMappings": null,
        "severity": "critical",
        "status": "stable",
        "suppressionDuration": "PT5H",
        "suppressionEnabled": false,
        "tactics": null,
        "techniques": [
          "t1555"
        ],
        "templateVersion": "1.0.0",
        "triggerThreshold": 0
      },
      "type": "Microsoft.OperationalInsights/workspaces/providers/alertRules"
    }
  ]
}
