{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "String"
    }
  },
  "resources": [
    {
      "apiVersion": "2023-02-01-preview",
      "id": "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/alertRules/05f3c945-dcc8-4393-9f3d-af65077a8f86')]",
      "kind": "Scheduled",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/05f3c945-dcc8-4393-9f3d-af65077a8f86')]",
      "properties": {
        "alertRuleTemplateName": "05f3c945-dcc8-4393-9f3d-af65077a8f86",
        "author": "Markus Neis, Jonhnathan Ribeiro, oscd.community",
        "customDetails": null,
        "date": "2018/04/09",
        "description": "Detects Access to Domain Group Policies stored in SYSVOL",
        "displayName": "Suspicious SYSVOL Domain Group Policy Access",
        "enabled": true,
        "entityMappings": null,
        "eventGroupingSettings": {
          "aggregationKind": "SingleAlert"
        },
        "logsource": {
          "category": "process_creation",
          "product": "windows"
        },
        "modified": "2022/01/07",
        "query": "DeviceProcessEvents\n| where ProcessCommandLine contains \"\\\\SYSVOL\\\\\" and ProcessCommandLine contains \"\\\\policies\\\\\"",
        "queryFrequency": "P1D",
        "queryPeriod": "P1D",
        "sentinelEntitiesMappings": null,
        "severity": "medium",
        "status": "test",
        "suppressionDuration": "PT5H",
        "suppressionEnabled": false,
        "tactics": null,
        "techniques": [
          "t1552"
        ],
        "templateVersion": "1.0.0",
        "triggerThreshold": 0
      },
      "type": "Microsoft.OperationalInsights/workspaces/providers/alertRules"
    }
  ]
}
