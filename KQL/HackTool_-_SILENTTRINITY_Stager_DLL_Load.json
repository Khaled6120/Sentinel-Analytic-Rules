{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "String"
    }
  },
  "resources": [
    {
      "apiVersion": "2023-02-01-preview",
      "id": "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/alertRules/75c505b1-711d-4f68-a357-8c3fe37dbf2d')]",
      "kind": "Scheduled",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/75c505b1-711d-4f68-a357-8c3fe37dbf2d')]",
      "properties": {
        "alertRuleTemplateName": "75c505b1-711d-4f68-a357-8c3fe37dbf2d",
        "author": "Aleksey Potapov, oscd.community",
        "customDetails": null,
        "date": "2019/10/22",
        "description": "Detects SILENTTRINITY stager dll loading activity",
        "displayName": "HackTool - SILENTTRINITY Stager DLL Load",
        "enabled": true,
        "entityMappings": null,
        "eventGroupingSettings": {
          "aggregationKind": "SingleAlert"
        },
        "logsource": {
          "category": "image_load",
          "product": "windows"
        },
        "modified": "2023/02/17",
        "query": "DeviceImageLoadEvents\n| where InitiatingProcessVersionInfoFileDescription contains \"st2stager\"",
        "queryFrequency": "P1D",
        "queryPeriod": "P1D",
        "sentinelEntitiesMappings": null,
        "severity": "high",
        "status": "test",
        "suppressionDuration": "PT5H",
        "suppressionEnabled": false,
        "tactics": null,
        "techniques": [
          "t1071"
        ],
        "templateVersion": "1.0.0",
        "triggerThreshold": 0
      },
      "type": "Microsoft.OperationalInsights/workspaces/providers/alertRules"
    }
  ]
}
