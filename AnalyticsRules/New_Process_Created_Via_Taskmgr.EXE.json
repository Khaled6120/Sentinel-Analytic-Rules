{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "String"
    }
  },
  "resources": [
    {
      "apiVersion": "2023-02-01-preview",
      "id": "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/alertRules/3d7679bd-0c00-440c-97b0-3f204273e6c7')]",
      "kind": "NRT",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/3d7679bd-0c00-440c-97b0-3f204273e6c7')]",
      "properties": {
        "alertRuleTemplateName": "3d7679bd-0c00-440c-97b0-3f204273e6c7",
        "author": "Florian Roth (Nextron Systems)",
        "customDetails": null,
        "date": "2018/03/13",
        "description": "Detects the creation of a process via the Windows task manager. This might be an attempt to bypass UAC",
        "displayName": "New Process Created Via Taskmgr.EXE",
        "enabled": true,
        "entityMappings": null,
        "eventGroupingSettings": {
          "aggregationKind": "SingleAlert"
        },
        "logsource": {
          "category": "process_creation",
          "product": "windows"
        },
        "modified": "2024/01/18",
        "query": "DeviceProcessEvents\n| where InitiatingProcessFolderPath endswith \"\\\\taskmgr.exe\" and (not((FolderPath endswith \":\\\\Windows\\\\System32\\\\mmc.exe\" or FolderPath endswith \":\\\\Windows\\\\System32\\\\resmon.exe\" or FolderPath endswith \":\\\\Windows\\\\System32\\\\Taskmgr.exe\")))",
        "queryFrequency": "P1D",
        "queryPeriod": "P1D",
        "sentinelEntitiesMappings": null,
        "severity": "low",
        "status": "test",
        "suppressionDuration": "PT5H",
        "suppressionEnabled": false,
        "tactics": null,
        "techniques": [
          "t1036"
        ],
        "templateVersion": "1.0.0",
        "triggerOperator": "GreaterThan",
        "triggerThreshold": 0
      },
      "type": "Microsoft.OperationalInsights/workspaces/providers/alertRules"
    }
  ]
}
