{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "String"
    }
  },
  "resources": [
    {
      "apiVersion": "2023-02-01-preview",
      "id": "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/alertRules/6e78b74f-c762-4800-82ad-f66787f10c8a')]",
      "kind": "NRT",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/6e78b74f-c762-4800-82ad-f66787f10c8a')]",
      "properties": {
        "alertRuleTemplateName": "6e78b74f-c762-4800-82ad-f66787f10c8a",
        "author": "X__Junior (Nextron Systems)",
        "customDetails": null,
        "date": "2023/03/13",
        "description": "Detects potential DLL sideloading of rcdll.dll",
        "displayName": "Potential Rcdll.DLL Sideloading",
        "enabled": true,
        "entityMappings": null,
        "eventGroupingSettings": {
          "aggregationKind": "SingleAlert"
        },
        "logsource": {
          "category": "image_load",
          "product": "windows"
        },
        "modified": "2023/03/15",
        "query": "DeviceImageLoadEvents\n| where FolderPath endswith \"\\\\rcdll.dll\" and (not((FolderPath startswith \"C:\\\\Program Files (x86)\\\\Microsoft Visual Studio\\\\\" or FolderPath startswith \"C:\\\\Program Files (x86)\\\\Windows Kits\\\\\")))",
        "queryFrequency": "P1D",
        "queryPeriod": "P1D",
        "sentinelEntitiesMappings": null,
        "severity": "high",
        "status": "test",
        "suppressionDuration": "PT5H",
        "suppressionEnabled": false,
        "tactics": null,
        "techniques": [
          "t1574"
        ],
        "templateVersion": "1.0.0",
        "triggerOperator": "GreaterThan",
        "triggerThreshold": 0
      },
      "type": "Microsoft.OperationalInsights/workspaces/providers/alertRules"
    }
  ]
}
