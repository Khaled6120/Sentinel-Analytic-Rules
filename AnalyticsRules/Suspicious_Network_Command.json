{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "String"
    }
  },
  "resources": [
    {
      "apiVersion": "2023-02-01-preview",
      "id": "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/alertRules/a29c1813-ab1f-4dde-b489-330b952e91ae')]",
      "kind": "Scheduled",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/a29c1813-ab1f-4dde-b489-330b952e91ae')]",
      "properties": {
        "alertRuleTemplateName": "a29c1813-ab1f-4dde-b489-330b952e91ae",
        "author": "frack113, Christopher Peacock '@securepeacock', SCYTHE '@scythe_io'",
        "customDetails": null,
        "date": "2021/12/07",
        "description": "Adversaries may look for details about the network configuration and settings of systems they access or through information discovery of remote systems",
        "displayName": "Suspicious Network Command",
        "enabled": true,
        "entityMappings": null,
        "eventGroupingSettings": {
          "aggregationKind": "SingleAlert"
        },
        "logsource": {
          "category": "process_creation",
          "product": "windows"
        },
        "modified": "2022/04/11",
        "query": "DeviceProcessEvents\n| where ProcessCommandLine contains \"ipconfig /all\" or ProcessCommandLine contains \"netsh interface show interface\" or ProcessCommandLine contains \"arp -a\" or ProcessCommandLine contains \"nbtstat -n\" or ProcessCommandLine contains \"net config\" or ProcessCommandLine contains \"route print\"",
        "queryFrequency": "P1D",
        "queryPeriod": "P1D",
        "sentinelEntitiesMappings": null,
        "severity": "low",
        "status": "test",
        "suppressionDuration": "PT5H",
        "suppressionEnabled": false,
        "tactics": [
          "discovery"
        ],
        "techniques": [
          "T1016"
        ],
        "templateVersion": "1.0.0",
        "triggerOperator": "GreaterThan",
        "triggerThreshold": 0
      },
      "type": "Microsoft.OperationalInsights/workspaces/providers/alertRules"
    }
  ]
}
