{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "String"
    }
  },
  "resources": [
    {
      "apiVersion": "2023-02-01-preview",
      "id": "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/alertRules/f38a82d2-fba3-4781-b549-525efbec8506')]",
      "kind": "NRT",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/f38a82d2-fba3-4781-b549-525efbec8506')]",
      "properties": {
        "alertRuleTemplateName": "f38a82d2-fba3-4781-b549-525efbec8506",
        "author": "Florian Roth (Nextron Systems)",
        "customDetails": null,
        "date": "2022/09/13",
        "description": "Detects the use of 3proxy, a tiny free proxy server",
        "displayName": "PUA - 3Proxy Execution",
        "enabled": true,
        "entityMappings": null,
        "eventGroupingSettings": {
          "aggregationKind": "SingleAlert"
        },
        "logsource": {
          "category": "process_creation",
          "product": "windows"
        },
        "modified": "2023/02/21",
        "query": "DeviceProcessEvents\n| where FolderPath endswith \"\\\\3proxy.exe\" or ProcessCommandLine contains \".exe -i127.0.0.1 -p\" or ProcessVersionInfoFileDescription =~ \"3proxy - tiny proxy server\"",
        "queryFrequency": "P1D",
        "queryPeriod": "P1D",
        "sentinelEntitiesMappings": null,
        "severity": "high",
        "status": "test",
        "suppressionDuration": "PT5H",
        "suppressionEnabled": false,
        "tactics": null,
        "techniques": [
          "t1572"
        ],
        "templateVersion": "1.0.0",
        "triggerOperator": "GreaterThan",
        "triggerThreshold": 0
      },
      "type": "Microsoft.OperationalInsights/workspaces/providers/alertRules"
    }
  ]
}
