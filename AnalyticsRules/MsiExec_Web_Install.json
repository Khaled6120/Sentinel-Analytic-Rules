{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "String"
    }
  },
  "resources": [
    {
      "apiVersion": "2023-02-01-preview",
      "id": "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/alertRules/f7b5f842-a6af-4da5-9e95-e32478f3cd2f')]",
      "kind": "Scheduled",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/f7b5f842-a6af-4da5-9e95-e32478f3cd2f')]",
      "properties": {
        "alertRuleTemplateName": "f7b5f842-a6af-4da5-9e95-e32478f3cd2f",
        "author": "Florian Roth (Nextron Systems)",
        "customDetails": null,
        "date": "2018/02/09",
        "description": "Detects suspicious msiexec process starts with web addresses as parameter",
        "displayName": "MsiExec Web Install",
        "enabled": true,
        "entityMappings": null,
        "eventGroupingSettings": {
          "aggregationKind": "SingleAlert"
        },
        "logsource": {
          "category": "process_creation",
          "product": "windows"
        },
        "modified": "2022/01/07",
        "query": "DeviceProcessEvents\n| where ProcessCommandLine contains \" msiexec\" and ProcessCommandLine contains \"://\"",
        "queryFrequency": "P1D",
        "queryPeriod": "P1D",
        "sentinelEntitiesMappings": null,
        "severity": "medium",
        "status": "test",
        "suppressionDuration": "PT5H",
        "suppressionEnabled": false,
        "tactics": null,
        "techniques": [
          "t1105",
          "t1218"
        ],
        "templateVersion": "1.0.0",
        "triggerThreshold": 0
      },
      "type": "Microsoft.OperationalInsights/workspaces/providers/alertRules"
    }
  ]
}
