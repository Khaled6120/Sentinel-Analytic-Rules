{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "String"
    }
  },
  "resources": [
    {
      "apiVersion": "2023-02-01-preview",
      "id": "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/alertRules/d679950c-abb7-43a6-80fb-2a480c4fc450')]",
      "kind": "NRT",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/d679950c-abb7-43a6-80fb-2a480c4fc450')]",
      "properties": {
        "alertRuleTemplateName": "d679950c-abb7-43a6-80fb-2a480c4fc450",
        "author": "frack113",
        "customDetails": null,
        "date": "2022/10/01",
        "description": "Detect use of PDQ Deploy remote admin tool",
        "displayName": "PDQ Deploy Remote Adminstartion Tool Execution",
        "enabled": true,
        "entityMappings": null,
        "eventGroupingSettings": {
          "aggregationKind": "SingleAlert"
        },
        "logsource": {
          "category": "process_creation",
          "product": "windows"
        },
        "modified": "2023/01/30",
        "query": "DeviceProcessEvents\n| where ProcessVersionInfoFileDescription =~ \"PDQ Deploy Console\" or ProcessVersionInfoProductName =~ \"PDQ Deploy\" or ProcessVersionInfoCompanyName =~ \"PDQ.com\" or ProcessVersionInfoOriginalFileName =~ \"PDQDeployConsole.exe\"",
        "queryFrequency": "P1D",
        "queryPeriod": "P1D",
        "sentinelEntitiesMappings": null,
        "severity": "medium",
        "status": "test",
        "suppressionDuration": "PT5H",
        "suppressionEnabled": false,
        "tactics": [
          "execution"
        ],
        "techniques": [
          "t1072"
        ],
        "templateVersion": "1.0.0",
        "triggerOperator": "GreaterThan",
        "triggerThreshold": 0
      },
      "type": "Microsoft.OperationalInsights/workspaces/providers/alertRules"
    }
  ]
}
