{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "String"
    }
  },
  "resources": [
    {
      "apiVersion": "2023-02-01-preview",
      "id": "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/alertRules/e2e80da2-8c66-4e00-ae3c-2eebd29f6b6d')]",
      "kind": "Scheduled",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/e2e80da2-8c66-4e00-ae3c-2eebd29f6b6d')]",
      "properties": {
        "alertRuleTemplateName": "e2e80da2-8c66-4e00-ae3c-2eebd29f6b6d",
        "author": "frack113",
        "customDetails": null,
        "date": "2021/07/27",
        "description": "An adversary may compress or encrypt data that is collected prior to exfiltration using 3rd party utilities",
        "displayName": "Compress Data and Lock With Password for Exfiltration With WINZIP",
        "enabled": true,
        "entityMappings": null,
        "eventGroupingSettings": {
          "aggregationKind": "SingleAlert"
        },
        "logsource": {
          "category": "process_creation",
          "product": "windows"
        },
        "modified": "2022/12/25",
        "query": "DeviceProcessEvents\n| where (ProcessCommandLine contains \" -min \" or ProcessCommandLine contains \" -a \") and ProcessCommandLine contains \"-s\\\"\" and (ProcessCommandLine contains \"winzip.exe\" or ProcessCommandLine contains \"winzip64.exe\")",
        "queryFrequency": "P1D",
        "queryPeriod": "P1D",
        "sentinelEntitiesMappings": null,
        "severity": "medium",
        "status": "test",
        "suppressionDuration": "PT5H",
        "suppressionEnabled": false,
        "tactics": [
          "collection"
        ],
        "techniques": [
          "T1560"
        ],
        "templateVersion": "1.0.0",
        "triggerOperator": "GreaterThan",
        "triggerThreshold": 0
      },
      "type": "Microsoft.OperationalInsights/workspaces/providers/alertRules"
    }
  ]
}
