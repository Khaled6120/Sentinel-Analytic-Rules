{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "String"
    }
  },
  "resources": [
    {
      "apiVersion": "2023-02-01-preview",
      "id": "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/alertRules/36475a7d-0f6d-4dce-9b01-6aeb473bbaf1')]",
      "kind": "NRT",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/36475a7d-0f6d-4dce-9b01-6aeb473bbaf1')]",
      "properties": {
        "alertRuleTemplateName": "36475a7d-0f6d-4dce-9b01-6aeb473bbaf1",
        "author": "frack113",
        "customDetails": null,
        "date": "2021/07/16",
        "description": "Executes arbitrary PowerShell code using SyncAppvPublishingServer.vbs",
        "displayName": "SyncAppvPublishingServer VBS Execute Arbitrary PowerShell Code",
        "enabled": true,
        "entityMappings": null,
        "eventGroupingSettings": {
          "aggregationKind": "SingleAlert"
        },
        "logsource": {
          "category": "process_creation",
          "product": "windows"
        },
        "modified": "2022/06/22",
        "query": "DeviceProcessEvents\n| where ProcessCommandLine contains \"\\\\SyncAppvPublishingServer.vbs\" and ProcessCommandLine contains \";\"",
        "queryFrequency": "P1D",
        "queryPeriod": "P1D",
        "sentinelEntitiesMappings": null,
        "severity": "medium",
        "status": "test",
        "suppressionDuration": "PT5H",
        "suppressionEnabled": false,
        "tactics": null,
        "techniques": [
          "t1216",
          "t1218"
        ],
        "templateVersion": "1.0.0",
        "triggerOperator": "GreaterThan",
        "triggerThreshold": 0
      },
      "type": "Microsoft.OperationalInsights/workspaces/providers/alertRules"
    }
  ]
}
