{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "String"
    }
  },
  "resources": [
    {
      "apiVersion": "2023-02-01-preview",
      "id": "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/alertRules/9637e8a5-7131-4f7f-bdc7-2b05d8670c43')]",
      "kind": "Scheduled",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/9637e8a5-7131-4f7f-bdc7-2b05d8670c43')]",
      "properties": {
        "alertRuleTemplateName": "9637e8a5-7131-4f7f-bdc7-2b05d8670c43",
        "author": "Markus Neis, Sander Wiebing",
        "customDetails": null,
        "date": "2018/11/22",
        "description": "Detects Executables in the Downloads folder without FileVersion,Description,Product,Company likely created with py2exe",
        "displayName": "Suspicious File Characteristics Due to Missing Fields",
        "enabled": true,
        "entityMappings": null,
        "eventGroupingSettings": {
          "aggregationKind": "SingleAlert"
        },
        "logsource": {
          "category": "process_creation",
          "product": "windows"
        },
        "modified": "2022/10/09",
        "query": "DeviceProcessEvents\n| where ((ProcessVersionInfoFileDescription =~ \"?\" and ProcessVersionInfoProductVersion =~ \"?\") or (ProcessVersionInfoFileDescription =~ \"?\" and ProcessVersionInfoProductName =~ \"?\") or (ProcessVersionInfoCompanyName =~ \"?\" and ProcessVersionInfoFileDescription =~ \"?\")) and FolderPath contains \"\\\\Downloads\\\\\"",
        "queryFrequency": "P1D",
        "queryPeriod": "P1D",
        "sentinelEntitiesMappings": null,
        "severity": "medium",
        "status": "test",
        "suppressionDuration": "PT5H",
        "suppressionEnabled": false,
        "tactics": [
          "execution"
        ],
        "techniques": [
          "t1059"
        ],
        "templateVersion": "1.0.0",
        "triggerThreshold": 0
      },
      "type": "Microsoft.OperationalInsights/workspaces/providers/alertRules"
    }
  ]
}
