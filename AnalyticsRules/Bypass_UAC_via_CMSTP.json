{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "String"
    }
  },
  "resources": [
    {
      "apiVersion": "2023-02-01-preview",
      "id": "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/alertRules/e66779cc-383e-4224-a3a4-267eeb585c40')]",
      "kind": "NRT",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/e66779cc-383e-4224-a3a4-267eeb585c40')]",
      "properties": {
        "alertRuleTemplateName": "e66779cc-383e-4224-a3a4-267eeb585c40",
        "author": "E.M. Anhaus (originally from Atomic Blue Detections, Endgame), oscd.community",
        "customDetails": null,
        "date": "2019/10/24",
        "description": "Detect commandline usage of Microsoft Connection Manager Profile Installer (cmstp.exe) to install specially formatted local .INF files",
        "displayName": "Bypass UAC via CMSTP",
        "enabled": true,
        "entityMappings": null,
        "eventGroupingSettings": {
          "aggregationKind": "SingleAlert"
        },
        "logsource": {
          "category": "process_creation",
          "product": "windows"
        },
        "modified": "2022/08/30",
        "query": "DeviceProcessEvents\n| where (ProcessCommandLine contains \"/s\" or ProcessCommandLine contains \"-s\" or ProcessCommandLine contains \"/au\" or ProcessCommandLine contains \"-au\" or ProcessCommandLine contains \"/ni\" or ProcessCommandLine contains \"-ni\") and (FolderPath endswith \"\\\\cmstp.exe\" or ProcessVersionInfoOriginalFileName =~ \"CMSTP.EXE\")",
        "queryFrequency": "P1D",
        "queryPeriod": "P1D",
        "sentinelEntitiesMappings": null,
        "severity": "high",
        "status": "test",
        "suppressionDuration": "PT5H",
        "suppressionEnabled": false,
        "tactics": null,
        "techniques": [
          "t1218",
          "t1548"
        ],
        "templateVersion": "1.0.0",
        "triggerOperator": "GreaterThan",
        "triggerThreshold": 0
      },
      "type": "Microsoft.OperationalInsights/workspaces/providers/alertRules"
    }
  ]
}
