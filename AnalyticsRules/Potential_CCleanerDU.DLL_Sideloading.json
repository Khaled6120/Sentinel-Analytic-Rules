{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "String"
    }
  },
  "resources": [
    {
      "apiVersion": "2023-02-01-preview",
      "id": "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/alertRules/1fbc0671-5596-4e17-8682-f020a0b995dc')]",
      "kind": "NRT",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/1fbc0671-5596-4e17-8682-f020a0b995dc')]",
      "properties": {
        "alertRuleTemplateName": "1fbc0671-5596-4e17-8682-f020a0b995dc",
        "author": "X__Junior (Nextron Systems)",
        "customDetails": null,
        "date": "2023/07/13",
        "description": "Detects potential DLL sideloading of \"CCleanerDU.dll\"",
        "displayName": "Potential CCleanerDU.DLL Sideloading",
        "enabled": true,
        "entityMappings": null,
        "eventGroupingSettings": {
          "aggregationKind": "SingleAlert"
        },
        "logsource": {
          "category": "image_load",
          "product": "windows"
        },
        "modified": "",
        "query": "DeviceImageLoadEvents\n| where FolderPath endswith \"\\\\CCleanerDU.dll\" and (not(((InitiatingProcessFolderPath endswith \"\\\\CCleaner.exe\" or InitiatingProcessFolderPath endswith \"\\\\CCleaner64.exe\") and (InitiatingProcessFolderPath startswith \"C:\\\\Program Files\\\\CCleaner\\\\\" or InitiatingProcessFolderPath startswith \"C:\\\\Program Files (x86)\\\\CCleaner\\\\\"))))",
        "queryFrequency": "P1D",
        "queryPeriod": "P1D",
        "sentinelEntitiesMappings": null,
        "severity": "medium",
        "status": "test",
        "suppressionDuration": "PT5H",
        "suppressionEnabled": false,
        "tactics": [
          "persistence"
        ],
        "techniques": [
          "t1574"
        ],
        "templateVersion": "1.0.0",
        "triggerOperator": "GreaterThan",
        "triggerThreshold": 0
      },
      "type": "Microsoft.OperationalInsights/workspaces/providers/alertRules"
    }
  ]
}
