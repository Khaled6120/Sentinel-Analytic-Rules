{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "String"
    }
  },
  "resources": [
    {
      "apiVersion": "2023-02-01-preview",
      "id": "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/alertRules/9eb271b9-24ae-4cd4-9465-19cfc1047f3e')]",
      "kind": "NRT",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/9eb271b9-24ae-4cd4-9465-19cfc1047f3e')]",
      "properties": {
        "alertRuleTemplateName": "9eb271b9-24ae-4cd4-9465-19cfc1047f3e",
        "author": "Furkan CALISKAN, @caliskanfurkan_, @oscd_initiative",
        "customDetails": null,
        "date": "2020/10/05",
        "description": "Attackers can use explorer.exe for evading defense mechanisms",
        "displayName": "Proxy Execution Via Explorer.exe",
        "enabled": true,
        "entityMappings": null,
        "eventGroupingSettings": {
          "aggregationKind": "SingleAlert"
        },
        "logsource": {
          "category": "process_creation",
          "product": "windows"
        },
        "modified": "2021/11/27",
        "query": "DeviceProcessEvents\n| where ProcessCommandLine contains \"explorer.exe\" and FolderPath endswith \"\\\\explorer.exe\" and InitiatingProcessFolderPath endswith \"\\\\cmd.exe\"",
        "queryFrequency": "P1D",
        "queryPeriod": "P1D",
        "sentinelEntitiesMappings": null,
        "severity": "low",
        "status": "test",
        "suppressionDuration": "PT5H",
        "suppressionEnabled": false,
        "tactics": null,
        "techniques": [
          "t1218"
        ],
        "templateVersion": "1.0.0",
        "triggerOperator": "GreaterThan",
        "triggerThreshold": 0
      },
      "type": "Microsoft.OperationalInsights/workspaces/providers/alertRules"
    }
  ]
}
