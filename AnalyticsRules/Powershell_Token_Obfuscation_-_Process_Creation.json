{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "String"
    }
  },
  "resources": [
    {
      "apiVersion": "2023-02-01-preview",
      "id": "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/alertRules/deb9b646-a508-44ee-b7c9-d8965921c6b6')]",
      "kind": "Scheduled",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/deb9b646-a508-44ee-b7c9-d8965921c6b6')]",
      "properties": {
        "alertRuleTemplateName": "deb9b646-a508-44ee-b7c9-d8965921c6b6",
        "author": "frack113",
        "customDetails": null,
        "date": "2022/12/27",
        "description": "Detects TOKEN OBFUSCATION technique from Invoke-Obfuscation",
        "displayName": "Powershell Token Obfuscation - Process Creation",
        "enabled": true,
        "entityMappings": null,
        "eventGroupingSettings": {
          "aggregationKind": "SingleAlert"
        },
        "logsource": {
          "category": "process_creation",
          "product": "windows"
        },
        "modified": "2022/12/30",
        "query": "DeviceProcessEvents\n| where ProcessCommandLine matches regex \"\\\\w+`(\\\\w+|-|.)`[\\\\w+|\\\\s]\" or ProcessCommandLine matches regex \"\"(\\\\{\\\\d\\\\})+\"\\\\s*-f\" or ProcessCommandLine matches regex \"\\\\$\\\\{((e|n|v)*`(e|n|v)*)+:path\\\\}|\\\\$\\\\{((e|n|v)*`(e|n|v)*)+:((p|a|t|h)*`(p|a|t|h)*)+\\\\}|\\\\$\\\\{env:((p|a|t|h)*`(p|a|t|h)*)+\\\\}\"",
        "queryFrequency": "P1D",
        "queryPeriod": "P1D",
        "sentinelEntitiesMappings": null,
        "severity": "high",
        "status": "test",
        "suppressionDuration": "PT5H",
        "suppressionEnabled": false,
        "tactics": null,
        "techniques": [
          "T1027"
        ],
        "templateVersion": "1.0.0",
        "triggerOperator": "GreaterThan",
        "triggerThreshold": 0
      },
      "type": "Microsoft.OperationalInsights/workspaces/providers/alertRules"
    }
  ]
}
