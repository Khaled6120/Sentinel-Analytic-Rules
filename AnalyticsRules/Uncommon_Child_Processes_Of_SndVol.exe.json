{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "String"
    }
  },
  "resources": [
    {
      "apiVersion": "2023-02-01-preview",
      "id": "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/alertRules/ba42babc-0666-4393-a4f7-ceaf5a69191e')]",
      "kind": "Scheduled",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/ba42babc-0666-4393-a4f7-ceaf5a69191e')]",
      "properties": {
        "alertRuleTemplateName": "ba42babc-0666-4393-a4f7-ceaf5a69191e",
        "author": "X__Junior (Nextron Systems)",
        "customDetails": null,
        "date": "2023/06/09",
        "description": "Detects potentially uncommon child processes of SndVol.exe (the Windows volume mixer)",
        "displayName": "Uncommon Child Processes Of SndVol.exe",
        "enabled": true,
        "entityMappings": null,
        "eventGroupingSettings": {
          "aggregationKind": "SingleAlert"
        },
        "logsource": {
          "category": "process_creation",
          "product": "windows"
        },
        "modified": "",
        "query": "DeviceProcessEvents\n| where InitiatingProcessFolderPath endswith \"\\\\SndVol.exe\" and (not((ProcessCommandLine contains \" shell32.dll,Control_RunDLL \" and FolderPath endswith \"\\\\rundll32.exe\")))",
        "queryFrequency": "P1D",
        "queryPeriod": "P1D",
        "sentinelEntitiesMappings": null,
        "severity": "medium",
        "status": "test",
        "suppressionDuration": "PT5H",
        "suppressionEnabled": false,
        "tactics": [
          "Execution"
        ],
        "techniques": [],
        "templateVersion": "1.0.0",
        "triggerOperator": "GreaterThan",
        "triggerThreshold": 0
      },
      "type": "Microsoft.OperationalInsights/workspaces/providers/alertRules"
    }
  ]
}
