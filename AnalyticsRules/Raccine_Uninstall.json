{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "String"
    }
  },
  "resources": [
    {
      "apiVersion": "2023-02-01-preview",
      "id": "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/alertRules/a31eeaed-3fd5-478e-a8ba-e62c6b3f9ecc')]",
      "kind": "Scheduled",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/a31eeaed-3fd5-478e-a8ba-e62c6b3f9ecc')]",
      "properties": {
        "alertRuleTemplateName": "a31eeaed-3fd5-478e-a8ba-e62c6b3f9ecc",
        "author": "Florian Roth (Nextron Systems)",
        "customDetails": null,
        "date": "2021/01/21",
        "description": "Detects commands that indicate a Raccine removal from an end system. Raccine is a free ransomware protection tool.",
        "displayName": "Raccine Uninstall",
        "enabled": true,
        "entityMappings": null,
        "eventGroupingSettings": {
          "aggregationKind": "SingleAlert"
        },
        "logsource": {
          "category": "process_creation",
          "product": "windows"
        },
        "modified": "2022/10/09",
        "query": "DeviceProcessEvents\n| where (ProcessCommandLine contains \"taskkill \" and ProcessCommandLine contains \"RaccineSettings.exe\") or (ProcessCommandLine contains \"reg.exe\" and ProcessCommandLine contains \"delete\" and ProcessCommandLine contains \"Raccine Tray\") or (ProcessCommandLine contains \"schtasks\" and ProcessCommandLine contains \"/DELETE\" and ProcessCommandLine contains \"Raccine Rules Updater\")",
        "queryFrequency": "P1D",
        "queryPeriod": "P1D",
        "sentinelEntitiesMappings": null,
        "severity": "high",
        "status": "test",
        "suppressionDuration": "PT5H",
        "suppressionEnabled": false,
        "tactics": null,
        "techniques": [
          "T1562"
        ],
        "templateVersion": "1.0.0",
        "triggerOperator": "GreaterThan",
        "triggerThreshold": 0
      },
      "type": "Microsoft.OperationalInsights/workspaces/providers/alertRules"
    }
  ]
}
