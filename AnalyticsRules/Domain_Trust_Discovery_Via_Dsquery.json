{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "String"
    }
  },
  "resources": [
    {
      "apiVersion": "2023-02-01-preview",
      "id": "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/alertRules/3bad990e-4848-4a78-9530-b427d854aac0')]",
      "kind": "Scheduled",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/3bad990e-4848-4a78-9530-b427d854aac0')]",
      "properties": {
        "alertRuleTemplateName": "3bad990e-4848-4a78-9530-b427d854aac0",
        "author": "E.M. Anhaus, Tony Lambert, oscd.community, omkar72",
        "customDetails": null,
        "date": "2019/10/24",
        "description": "Detects execution of \"dsquery.exe\" for domain trust discovery",
        "displayName": "Domain Trust Discovery Via Dsquery",
        "enabled": true,
        "entityMappings": null,
        "eventGroupingSettings": {
          "aggregationKind": "SingleAlert"
        },
        "logsource": {
          "category": "process_creation",
          "product": "windows"
        },
        "modified": "2023/02/02",
        "query": "DeviceProcessEvents\n| where ProcessCommandLine contains \"trustedDomain\" and (FolderPath endswith \"\\\\dsquery.exe\" or ProcessVersionInfoOriginalFileName =~ \"dsquery.exe\")",
        "queryFrequency": "P1D",
        "queryPeriod": "P1D",
        "sentinelEntitiesMappings": null,
        "severity": "medium",
        "status": "test",
        "suppressionDuration": "PT5H",
        "suppressionEnabled": false,
        "tactics": [
          "Discovery"
        ],
        "techniques": [
          "T1482"
        ],
        "templateVersion": "1.0.0",
        "triggerOperator": "GreaterThan",
        "triggerThreshold": 0
      },
      "type": "Microsoft.OperationalInsights/workspaces/providers/alertRules"
    }
  ]
}
