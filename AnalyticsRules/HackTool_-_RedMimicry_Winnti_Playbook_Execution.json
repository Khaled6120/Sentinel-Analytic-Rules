{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "String"
    }
  },
  "resources": [
    {
      "apiVersion": "2023-02-01-preview",
      "id": "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/alertRules/95022b85-ff2a-49fa-939a-d7b8f56eeb9b')]",
      "kind": "Scheduled",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/95022b85-ff2a-49fa-939a-d7b8f56eeb9b')]",
      "properties": {
        "alertRuleTemplateName": "95022b85-ff2a-49fa-939a-d7b8f56eeb9b",
        "author": "Alexander Rausch",
        "customDetails": null,
        "date": "2020/06/24",
        "description": "Detects actions caused by the RedMimicry Winnti playbook a automated breach emulations utility",
        "displayName": "HackTool - RedMimicry Winnti Playbook Execution",
        "enabled": true,
        "entityMappings": null,
        "eventGroupingSettings": {
          "aggregationKind": "SingleAlert"
        },
        "logsource": {
          "category": "process_creation",
          "product": "windows"
        },
        "modified": "2023/03/01",
        "query": "DeviceProcessEvents\n| where (ProcessCommandLine contains \"gthread-3.6.dll\" or ProcessCommandLine contains \"\\\\Windows\\\\Temp\\\\tmp.bat\" or ProcessCommandLine contains \"sigcmm-2.4.dll\") and (FolderPath endswith \"\\\\rundll32.exe\" or FolderPath endswith \"\\\\cmd.exe\")",
        "queryFrequency": "P1D",
        "queryPeriod": "P1D",
        "sentinelEntitiesMappings": null,
        "severity": "high",
        "status": "test",
        "suppressionDuration": "PT5H",
        "suppressionEnabled": false,
        "tactics": [
          "execution"
        ],
        "techniques": [
          "t1059",
          "t1106",
          "t1218"
        ],
        "templateVersion": "1.0.0",
        "triggerThreshold": 0
      },
      "type": "Microsoft.OperationalInsights/workspaces/providers/alertRules"
    }
  ]
}
