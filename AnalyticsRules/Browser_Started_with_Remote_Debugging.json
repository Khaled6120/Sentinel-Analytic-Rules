{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "String"
    }
  },
  "resources": [
    {
      "apiVersion": "2023-02-01-preview",
      "id": "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/alertRules/b3d34dc5-2efd-4ae3-845f-8ec14921f449')]",
      "kind": "NRT",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/b3d34dc5-2efd-4ae3-845f-8ec14921f449')]",
      "properties": {
        "alertRuleTemplateName": "b3d34dc5-2efd-4ae3-845f-8ec14921f449",
        "author": "pH-T (Nextron Systems), Nasreddine Bencherchali (Nextron Systems)",
        "customDetails": null,
        "date": "2022/07/27",
        "description": "Detects browsers starting with the remote debugging flags. Which is a technique often used to perform browser injection attacks",
        "displayName": "Browser Started with Remote Debugging",
        "enabled": true,
        "entityMappings": null,
        "eventGroupingSettings": {
          "aggregationKind": "SingleAlert"
        },
        "logsource": {
          "category": "process_creation",
          "product": "windows"
        },
        "modified": "2022/12/23",
        "query": "DeviceProcessEvents\n| where ProcessCommandLine contains \" --remote-debugging-\" or (ProcessCommandLine contains \" -start-debugger-server\" and FolderPath endswith \"\\\\firefox.exe\")",
        "queryFrequency": "P1D",
        "queryPeriod": "P1D",
        "sentinelEntitiesMappings": null,
        "severity": "medium",
        "status": "test",
        "suppressionDuration": "PT5H",
        "suppressionEnabled": false,
        "tactics": null,
        "techniques": [
          "t1185"
        ],
        "templateVersion": "1.0.0",
        "triggerOperator": "GreaterThan",
        "triggerThreshold": 0
      },
      "type": "Microsoft.OperationalInsights/workspaces/providers/alertRules"
    }
  ]
}
