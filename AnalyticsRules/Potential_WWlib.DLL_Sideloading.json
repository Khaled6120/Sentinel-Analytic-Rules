{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "String"
    }
  },
  "resources": [
    {
      "apiVersion": "2023-02-01-preview",
      "id": "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/alertRules/e2e01011-5910-4267-9c3b-4149ed5479cf')]",
      "kind": "NRT",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/e2e01011-5910-4267-9c3b-4149ed5479cf')]",
      "properties": {
        "alertRuleTemplateName": "e2e01011-5910-4267-9c3b-4149ed5479cf",
        "author": "X__Junior (Nextron Systems)",
        "customDetails": null,
        "date": "2023/05/18",
        "description": "Detects potential DLL sideloading of \"wwlib.dll\"",
        "displayName": "Potential WWlib.DLL Sideloading",
        "enabled": true,
        "entityMappings": null,
        "eventGroupingSettings": {
          "aggregationKind": "SingleAlert"
        },
        "logsource": {
          "category": "image_load",
          "product": "windows"
        },
        "modified": "",
        "query": "DeviceImageLoadEvents\n| where FolderPath endswith \"\\\\wwlib.dll\" and (not(((FolderPath startswith \"C:\\\\Program Files (x86)\\\\Microsoft Office\\\\\" or FolderPath startswith \"C:\\\\Program Files\\\\Microsoft Office\\\\\") and InitiatingProcessFolderPath endswith \"\\\\winword.exe\" and (InitiatingProcessFolderPath startswith \"C:\\\\Program Files (x86)\\\\Microsoft Office\\\\\" or InitiatingProcessFolderPath startswith \"C:\\\\Program Files\\\\Microsoft Office\\\\\"))))",
        "queryFrequency": "P1D",
        "queryPeriod": "P1D",
        "sentinelEntitiesMappings": null,
        "severity": "medium",
        "status": "test",
        "suppressionDuration": "PT5H",
        "suppressionEnabled": false,
        "tactics": null,
        "techniques": [
          "t1574"
        ],
        "templateVersion": "1.0.0",
        "triggerOperator": "GreaterThan",
        "triggerThreshold": 0
      },
      "type": "Microsoft.OperationalInsights/workspaces/providers/alertRules"
    }
  ]
}
