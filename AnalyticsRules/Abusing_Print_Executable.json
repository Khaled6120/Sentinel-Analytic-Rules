{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "String"
    }
  },
  "resources": [
    {
      "apiVersion": "2023-02-01-preview",
      "id": "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/alertRules/bafac3d6-7de9-4dd9-8874-4a1194b493ed')]",
      "kind": "Scheduled",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/bafac3d6-7de9-4dd9-8874-4a1194b493ed')]",
      "properties": {
        "alertRuleTemplateName": "bafac3d6-7de9-4dd9-8874-4a1194b493ed",
        "author": "Furkan CALISKAN, @caliskanfurkan_, @oscd_initiative",
        "customDetails": null,
        "date": "2020/10/05",
        "description": "Attackers can use print.exe for remote file copy",
        "displayName": "Abusing Print Executable",
        "enabled": true,
        "entityMappings": null,
        "eventGroupingSettings": {
          "aggregationKind": "SingleAlert"
        },
        "logsource": {
          "category": "process_creation",
          "product": "windows"
        },
        "modified": "2022/07/07",
        "query": "DeviceProcessEvents\n| where ((ProcessCommandLine contains \"/D\" and ProcessCommandLine contains \".exe\") and ProcessCommandLine startswith \"print\" and FolderPath endswith \"\\\\print.exe\") and (not(ProcessCommandLine contains \"print.exe\"))",
        "queryFrequency": "P1D",
        "queryPeriod": "P1D",
        "sentinelEntitiesMappings": null,
        "severity": "medium",
        "status": "test",
        "suppressionDuration": "PT5H",
        "suppressionEnabled": false,
        "tactics": null,
        "techniques": [
          "T1218"
        ],
        "templateVersion": "1.0.0",
        "triggerOperator": "GreaterThan",
        "triggerThreshold": 0
      },
      "type": "Microsoft.OperationalInsights/workspaces/providers/alertRules"
    }
  ]
}
