{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "String"
    }
  },
  "resources": [
    {
      "apiVersion": "2023-02-01-preview",
      "id": "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/alertRules/b57ba453-b384-4ab9-9f40-1038086b4e53')]",
      "kind": "NRT",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/b57ba453-b384-4ab9-9f40-1038086b4e53')]",
      "properties": {
        "alertRuleTemplateName": "b57ba453-b384-4ab9-9f40-1038086b4e53",
        "author": "frack113",
        "customDetails": null,
        "date": "2021/12/20",
        "description": "Detects dump of credentials in VeeamBackup dbo",
        "displayName": "VeeamBackup Database Credentials Dump Via Sqlcmd.EXE",
        "enabled": true,
        "entityMappings": null,
        "eventGroupingSettings": {
          "aggregationKind": "SingleAlert"
        },
        "logsource": {
          "category": "process_creation",
          "product": "windows"
        },
        "modified": "2023/02/13",
        "query": "DeviceProcessEvents\n| where (ProcessCommandLine contains \"SELECT\" and ProcessCommandLine contains \"TOP\" and ProcessCommandLine contains \"[VeeamBackup].[dbo].[Credentials]\") and FolderPath endswith \"\\\\sqlcmd.exe\"",
        "queryFrequency": "P1D",
        "queryPeriod": "P1D",
        "sentinelEntitiesMappings": null,
        "severity": "high",
        "status": "test",
        "suppressionDuration": "PT5H",
        "suppressionEnabled": false,
        "tactics": [
          "collection"
        ],
        "techniques": [
          "t1005"
        ],
        "templateVersion": "1.0.0",
        "triggerOperator": "GreaterThan",
        "triggerThreshold": 0
      },
      "type": "Microsoft.OperationalInsights/workspaces/providers/alertRules"
    }
  ]
}
