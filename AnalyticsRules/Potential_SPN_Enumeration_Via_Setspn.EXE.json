{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "String"
    }
  },
  "resources": [
    {
      "apiVersion": "2023-02-01-preview",
      "id": "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/alertRules/1eeed653-dbc8-4187-ad0c-eeebb20e6599')]",
      "kind": "Scheduled",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/1eeed653-dbc8-4187-ad0c-eeebb20e6599')]",
      "properties": {
        "alertRuleTemplateName": "1eeed653-dbc8-4187-ad0c-eeebb20e6599",
        "author": "Markus Neis, keepwatch",
        "customDetails": null,
        "date": "2018/11/14",
        "description": "Detects service principal name (SPN) enumeration used for Kerberoasting",
        "displayName": "Potential SPN Enumeration Via Setspn.EXE",
        "enabled": true,
        "entityMappings": null,
        "eventGroupingSettings": {
          "aggregationKind": "SingleAlert"
        },
        "logsource": {
          "category": "process_creation",
          "product": "windows"
        },
        "modified": "2023/10/23",
        "query": "DeviceProcessEvents\n| where (ProcessCommandLine contains \" -q \" or ProcessCommandLine contains \" /q \") and (FolderPath endswith \"\\\\setspn.exe\" or ProcessVersionInfoOriginalFileName =~ \"setspn.exe\" or (ProcessVersionInfoFileDescription contains \"Query or reset the computer\" and ProcessVersionInfoFileDescription contains \"SPN attribute\"))",
        "queryFrequency": "P1D",
        "queryPeriod": "P1D",
        "sentinelEntitiesMappings": null,
        "severity": "medium",
        "status": "test",
        "suppressionDuration": "PT5H",
        "suppressionEnabled": false,
        "tactics": null,
        "techniques": [
          "t1558"
        ],
        "templateVersion": "1.0.0",
        "triggerThreshold": 0
      },
      "type": "Microsoft.OperationalInsights/workspaces/providers/alertRules"
    }
  ]
}
