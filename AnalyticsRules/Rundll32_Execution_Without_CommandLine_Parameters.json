{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "String"
    }
  },
  "resources": [
    {
      "apiVersion": "2023-02-01-preview",
      "id": "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/alertRules/1775e15e-b61b-4d14-a1a3-80981298085a')]",
      "kind": "Scheduled",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/1775e15e-b61b-4d14-a1a3-80981298085a')]",
      "properties": {
        "alertRuleTemplateName": "1775e15e-b61b-4d14-a1a3-80981298085a",
        "author": "Florian Roth (Nextron Systems)",
        "customDetails": null,
        "date": "2021/05/27",
        "description": "Detects suspicious start of rundll32.exe without any parameters as found in CobaltStrike beacon activity",
        "displayName": "Rundll32 Execution Without CommandLine Parameters",
        "enabled": true,
        "entityMappings": null,
        "eventGroupingSettings": {
          "aggregationKind": "SingleAlert"
        },
        "logsource": {
          "category": "process_creation",
          "product": "windows"
        },
        "modified": "2023/08/31",
        "query": "DeviceProcessEvents\n| where (ProcessCommandLine endswith \"\\\\rundll32.exe\" or ProcessCommandLine endswith \"\\\\rundll32.exe\\\"\" or ProcessCommandLine endswith \"\\\\rundll32\") and (not((InitiatingProcessFolderPath contains \"\\\\AppData\\\\Local\\\\\" or InitiatingProcessFolderPath contains \"\\\\Microsoft\\\\Edge\\\\\")))",
        "queryFrequency": "P1D",
        "queryPeriod": "P1D",
        "sentinelEntitiesMappings": null,
        "severity": "high",
        "status": "experimental",
        "suppressionDuration": "PT5H",
        "suppressionEnabled": false,
        "tactics": null,
        "techniques": [
          "t1202"
        ],
        "templateVersion": "1.0.0",
        "triggerThreshold": 0
      },
      "type": "Microsoft.OperationalInsights/workspaces/providers/alertRules"
    }
  ]
}
