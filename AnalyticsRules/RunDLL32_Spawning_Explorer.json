{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "String"
    }
  },
  "resources": [
    {
      "apiVersion": "2023-02-01-preview",
      "id": "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/alertRules/caa06de8-fdef-4c91-826a-7f9e163eef4b')]",
      "kind": "Scheduled",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/caa06de8-fdef-4c91-826a-7f9e163eef4b')]",
      "properties": {
        "alertRuleTemplateName": "caa06de8-fdef-4c91-826a-7f9e163eef4b",
        "author": "elhoim, CD_ROM_",
        "customDetails": null,
        "date": "2022/04/27",
        "description": "Detects RunDLL32.exe spawning explorer.exe as child, which is very uncommon, often observes Gamarue spawning the explorer.exe process in an unusual way",
        "displayName": "RunDLL32 Spawning Explorer",
        "enabled": true,
        "entityMappings": null,
        "eventGroupingSettings": {
          "aggregationKind": "SingleAlert"
        },
        "logsource": {
          "category": "process_creation",
          "product": "windows"
        },
        "modified": "2022/05/25",
        "query": "DeviceProcessEvents\n| where (FolderPath endswith \"\\\\explorer.exe\" and InitiatingProcessFolderPath endswith \"\\\\rundll32.exe\") and (not(InitiatingProcessCommandLine contains \"\\\\shell32.dll,Control_RunDLL\"))",
        "queryFrequency": "P1D",
        "queryPeriod": "P1D",
        "sentinelEntitiesMappings": null,
        "severity": "high",
        "status": "test",
        "suppressionDuration": "PT5H",
        "suppressionEnabled": false,
        "tactics": null,
        "techniques": [
          "T1218"
        ],
        "templateVersion": "1.0.0",
        "triggerOperator": "GreaterThan",
        "triggerThreshold": 0
      },
      "type": "Microsoft.OperationalInsights/workspaces/providers/alertRules"
    }
  ]
}
