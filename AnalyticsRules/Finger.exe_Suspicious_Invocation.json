{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "String"
    }
  },
  "resources": [
    {
      "apiVersion": "2023-02-01-preview",
      "id": "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/alertRules/af491bca-e752-4b44-9c86-df5680533dbc')]",
      "kind": "Scheduled",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/af491bca-e752-4b44-9c86-df5680533dbc')]",
      "properties": {
        "alertRuleTemplateName": "af491bca-e752-4b44-9c86-df5680533dbc",
        "author": "Florian Roth (Nextron Systems), omkar72, oscd.community",
        "customDetails": null,
        "date": "2021/02/24",
        "description": "Detects suspicious aged finger.exe tool execution often used in malware attacks nowadays",
        "displayName": "Finger.exe Suspicious Invocation",
        "enabled": true,
        "entityMappings": null,
        "eventGroupingSettings": {
          "aggregationKind": "SingleAlert"
        },
        "logsource": {
          "category": "process_creation",
          "product": "windows"
        },
        "modified": "2022/08/16",
        "query": "DeviceProcessEvents\n| where ProcessVersionInfoOriginalFileName =~ \"finger.exe\" or FolderPath endswith \"\\\\finger.exe\"",
        "queryFrequency": "P1D",
        "queryPeriod": "P1D",
        "sentinelEntitiesMappings": null,
        "severity": "high",
        "status": "test",
        "suppressionDuration": "PT5H",
        "suppressionEnabled": false,
        "tactics": null,
        "techniques": [
          "t1105"
        ],
        "templateVersion": "1.0.0",
        "triggerThreshold": 0
      },
      "type": "Microsoft.OperationalInsights/workspaces/providers/alertRules"
    }
  ]
}
