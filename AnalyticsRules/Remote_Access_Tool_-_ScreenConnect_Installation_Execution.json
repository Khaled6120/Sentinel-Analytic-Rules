{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "String"
    }
  },
  "resources": [
    {
      "apiVersion": "2023-02-01-preview",
      "id": "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/alertRules/75bfe6e6-cd8e-429e-91d3-03921e1d7962')]",
      "kind": "NRT",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/75bfe6e6-cd8e-429e-91d3-03921e1d7962')]",
      "properties": {
        "alertRuleTemplateName": "75bfe6e6-cd8e-429e-91d3-03921e1d7962",
        "author": "Florian Roth (Nextron Systems)",
        "customDetails": null,
        "date": "2021/02/11",
        "description": "Detects ScreenConnect program starts that establish a remote access to a system.",
        "displayName": "Remote Access Tool - ScreenConnect Installation Execution",
        "enabled": true,
        "entityMappings": null,
        "eventGroupingSettings": {
          "aggregationKind": "SingleAlert"
        },
        "logsource": {
          "category": "process_creation",
          "product": "windows"
        },
        "modified": "2024/02/26",
        "query": "DeviceProcessEvents\n| where ProcessCommandLine contains \"e=Access&\" and ProcessCommandLine contains \"y=Guest&\" and ProcessCommandLine contains \"&p=\" and ProcessCommandLine contains \"&c=\" and ProcessCommandLine contains \"&k=\"",
        "queryFrequency": "P1D",
        "queryPeriod": "P1D",
        "sentinelEntitiesMappings": null,
        "severity": "medium",
        "status": "test",
        "suppressionDuration": "PT5H",
        "suppressionEnabled": false,
        "tactics": null,
        "techniques": [
          "t1133"
        ],
        "templateVersion": "1.0.0",
        "triggerOperator": "GreaterThan",
        "triggerThreshold": 0
      },
      "type": "Microsoft.OperationalInsights/workspaces/providers/alertRules"
    }
  ]
}
