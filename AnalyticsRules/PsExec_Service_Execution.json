{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "String"
    }
  },
  "resources": [
    {
      "apiVersion": "2023-02-01-preview",
      "id": "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/alertRules/fdfcbd78-48f1-4a4b-90ac-d82241e368c5')]",
      "kind": "Scheduled",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/fdfcbd78-48f1-4a4b-90ac-d82241e368c5')]",
      "properties": {
        "alertRuleTemplateName": "fdfcbd78-48f1-4a4b-90ac-d82241e368c5",
        "author": "Thomas Patzke, Romaissa Adjailia, Florian Roth (Nextron Systems)",
        "customDetails": null,
        "date": "2017/06/12",
        "description": "Detects launch of the PSEXESVC service, which means that this system was the target of a psexec remote execution",
        "displayName": "PsExec Service Execution",
        "enabled": true,
        "entityMappings": null,
        "eventGroupingSettings": {
          "aggregationKind": "SingleAlert"
        },
        "logsource": {
          "category": "process_creation",
          "product": "windows"
        },
        "modified": "2023/02/28",
        "query": "DeviceProcessEvents\n| where FolderPath =~ \"C:\\\\Windows\\\\PSEXESVC.exe\" or ProcessVersionInfoOriginalFileName =~ \"psexesvc.exe\"",
        "queryFrequency": "P1D",
        "queryPeriod": "P1D",
        "sentinelEntitiesMappings": null,
        "severity": "medium",
        "status": "test",
        "suppressionDuration": "PT5H",
        "suppressionEnabled": false,
        "tactics": [
          "execution"
        ],
        "techniques": [],
        "templateVersion": "1.0.0",
        "triggerThreshold": 0
      },
      "type": "Microsoft.OperationalInsights/workspaces/providers/alertRules"
    }
  ]
}
