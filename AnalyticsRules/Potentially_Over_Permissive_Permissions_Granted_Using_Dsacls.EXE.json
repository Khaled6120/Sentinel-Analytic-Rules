{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "String"
    }
  },
  "resources": [
    {
      "apiVersion": "2023-02-01-preview",
      "id": "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/alertRules/01c42d3c-242d-4655-85b2-34f1739632f7')]",
      "kind": "Scheduled",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/01c42d3c-242d-4655-85b2-34f1739632f7')]",
      "properties": {
        "alertRuleTemplateName": "01c42d3c-242d-4655-85b2-34f1739632f7",
        "author": "Nasreddine Bencherchali (Nextron Systems)",
        "customDetails": null,
        "date": "2022/06/20",
        "description": "Detects usage of Dsacls to grant over permissive permissions",
        "displayName": "Potentially Over Permissive Permissions Granted Using Dsacls.EXE",
        "enabled": true,
        "entityMappings": null,
        "eventGroupingSettings": {
          "aggregationKind": "SingleAlert"
        },
        "logsource": {
          "category": "process_creation",
          "product": "windows"
        },
        "modified": "2023/02/04",
        "query": "DeviceProcessEvents\n| where ProcessCommandLine contains \" /G \" and (FolderPath endswith \"\\\\dsacls.exe\" or ProcessVersionInfoOriginalFileName =~ \"DSACLS.EXE\") and (ProcessCommandLine contains \"GR\" or ProcessCommandLine contains \"GE\" or ProcessCommandLine contains \"GW\" or ProcessCommandLine contains \"GA\" or ProcessCommandLine contains \"WP\" or ProcessCommandLine contains \"WD\")",
        "queryFrequency": "P1D",
        "queryPeriod": "P1D",
        "sentinelEntitiesMappings": null,
        "severity": "medium",
        "status": "test",
        "suppressionDuration": "PT5H",
        "suppressionEnabled": false,
        "tactics": null,
        "techniques": [
          "T1218"
        ],
        "templateVersion": "1.0.0",
        "triggerOperator": "GreaterThan",
        "triggerThreshold": 0
      },
      "type": "Microsoft.OperationalInsights/workspaces/providers/alertRules"
    }
  ]
}
