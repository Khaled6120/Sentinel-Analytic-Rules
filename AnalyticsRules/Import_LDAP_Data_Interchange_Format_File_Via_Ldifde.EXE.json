{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "String"
    }
  },
  "resources": [
    {
      "apiVersion": "2023-02-01-preview",
      "id": "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/alertRules/6f535e01-ca1f-40be-ab8d-45b19c0c8b7f')]",
      "kind": "Scheduled",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/6f535e01-ca1f-40be-ab8d-45b19c0c8b7f')]",
      "properties": {
        "alertRuleTemplateName": "6f535e01-ca1f-40be-ab8d-45b19c0c8b7f",
        "author": "@gott_cyber",
        "customDetails": null,
        "date": "2022/09/02",
        "description": "Detects the execution of \"Ldifde.exe\" with the import flag \"-i\". The can be abused to include HTTP-based arguments which will allow the arbitrary download of files from a remote server.\n",
        "displayName": "Import LDAP Data Interchange Format File Via Ldifde.EXE",
        "enabled": true,
        "entityMappings": null,
        "eventGroupingSettings": {
          "aggregationKind": "SingleAlert"
        },
        "logsource": {
          "category": "process_creation",
          "product": "windows"
        },
        "modified": "2023/03/14",
        "query": "DeviceProcessEvents\n| where (ProcessCommandLine contains \"-i\" and ProcessCommandLine contains \"-f\") and (FolderPath endswith \"\\\\ldifde.exe\" or ProcessVersionInfoOriginalFileName =~ \"ldifde.exe\")",
        "queryFrequency": "P1D",
        "queryPeriod": "P1D",
        "sentinelEntitiesMappings": null,
        "severity": "medium",
        "status": "test",
        "suppressionDuration": "PT5H",
        "suppressionEnabled": false,
        "tactics": null,
        "techniques": [
          "t1105",
          "t1218"
        ],
        "templateVersion": "1.0.0",
        "triggerThreshold": 0
      },
      "type": "Microsoft.OperationalInsights/workspaces/providers/alertRules"
    }
  ]
}
