{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "String"
    }
  },
  "resources": [
    {
      "apiVersion": "2023-02-01-preview",
      "id": "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/alertRules/b18c9d4c-fac9-4708-bd06-dd5bfacf200f')]",
      "kind": "Scheduled",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/b18c9d4c-fac9-4708-bd06-dd5bfacf200f')]",
      "properties": {
        "alertRuleTemplateName": "b18c9d4c-fac9-4708-bd06-dd5bfacf200f",
        "author": "Alfie Champion (ajpc500)",
        "customDetails": null,
        "date": "2021/06/02",
        "description": "F-Secure C3 produces DLLs with a default exported StartNodeRelay function.",
        "displayName": "HackTool - F-Secure C3 Load by Rundll32",
        "enabled": true,
        "entityMappings": null,
        "eventGroupingSettings": {
          "aggregationKind": "SingleAlert"
        },
        "logsource": {
          "category": "process_creation",
          "product": "windows"
        },
        "modified": "2023/03/05",
        "query": "DeviceProcessEvents\n| where ProcessCommandLine contains \"rundll32.exe\" and ProcessCommandLine contains \".dll\" and ProcessCommandLine contains \"StartNodeRelay\"",
        "queryFrequency": "P1D",
        "queryPeriod": "P1D",
        "sentinelEntitiesMappings": null,
        "severity": "critical",
        "status": "test",
        "suppressionDuration": "PT5H",
        "suppressionEnabled": false,
        "tactics": null,
        "techniques": [
          "T1218"
        ],
        "templateVersion": "1.0.0",
        "triggerOperator": "GreaterThan",
        "triggerThreshold": 0
      },
      "type": "Microsoft.OperationalInsights/workspaces/providers/alertRules"
    }
  ]
}
