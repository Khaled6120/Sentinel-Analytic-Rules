{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "String"
    }
  },
  "resources": [
    {
      "apiVersion": "2023-02-01-preview",
      "id": "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/alertRules/4ede543c-e098-43d9-a28f-dd784a13132f')]",
      "kind": "Scheduled",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/4ede543c-e098-43d9-a28f-dd784a13132f')]",
      "properties": {
        "alertRuleTemplateName": "4ede543c-e098-43d9-a28f-dd784a13132f",
        "author": "Florian Roth (Nextron Systems), Tigzy",
        "customDetails": null,
        "date": "2021/11/17",
        "description": "Detects a suspicious winrar execution in a folder which is not the default installation folder",
        "displayName": "Winrar Execution in Non-Standard Folder",
        "enabled": true,
        "entityMappings": null,
        "eventGroupingSettings": {
          "aggregationKind": "SingleAlert"
        },
        "logsource": {
          "category": "process_creation",
          "product": "windows"
        },
        "modified": "2023/08/31",
        "query": "DeviceProcessEvents\n| where ((FolderPath endswith \"\\\\rar.exe\" or FolderPath endswith \"\\\\winrar.exe\") or ProcessVersionInfoFileDescription =~ \"Command line RAR\") and (not(((FolderPath contains \":\\\\Program Files (x86)\\\\WinRAR\\\\\" or FolderPath contains \":\\\\Program Files\\\\WinRAR\\\\\") or FolderPath endswith \"\\\\UnRAR.exe\"))) and (not(FolderPath contains \":\\\\Windows\\\\Temp\\\\\"))",
        "queryFrequency": "P1D",
        "queryPeriod": "P1D",
        "sentinelEntitiesMappings": null,
        "severity": "medium",
        "status": "test",
        "suppressionDuration": "PT5H",
        "suppressionEnabled": false,
        "tactics": [
          "collection"
        ],
        "techniques": [
          "T1560"
        ],
        "templateVersion": "1.0.0",
        "triggerOperator": "GreaterThan",
        "triggerThreshold": 0
      },
      "type": "Microsoft.OperationalInsights/workspaces/providers/alertRules"
    }
  ]
}
