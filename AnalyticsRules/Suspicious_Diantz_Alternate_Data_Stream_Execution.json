{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "String"
    }
  },
  "resources": [
    {
      "apiVersion": "2023-02-01-preview",
      "id": "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/alertRules/6b369ced-4b1d-48f1-b427-fdc0de0790bd')]",
      "kind": "Scheduled",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/6b369ced-4b1d-48f1-b427-fdc0de0790bd')]",
      "properties": {
        "alertRuleTemplateName": "6b369ced-4b1d-48f1-b427-fdc0de0790bd",
        "author": "frack113",
        "customDetails": null,
        "date": "2021/11/26",
        "description": "Compress target file into a cab file stored in the Alternate Data Stream (ADS) of the target file.",
        "displayName": "Suspicious Diantz Alternate Data Stream Execution",
        "enabled": true,
        "entityMappings": null,
        "eventGroupingSettings": {
          "aggregationKind": "SingleAlert"
        },
        "logsource": {
          "category": "process_creation",
          "product": "windows"
        },
        "modified": "2022/12/31",
        "query": "DeviceProcessEvents\n| where (ProcessCommandLine contains \"diantz.exe\" and ProcessCommandLine contains \".cab\") and ProcessCommandLine matches regex \":[^\\\\\\\\]\"",
        "queryFrequency": "P1D",
        "queryPeriod": "P1D",
        "sentinelEntitiesMappings": null,
        "severity": "medium",
        "status": "test",
        "suppressionDuration": "PT5H",
        "suppressionEnabled": false,
        "tactics": null,
        "techniques": [
          "T1564"
        ],
        "templateVersion": "1.0.0",
        "triggerOperator": "GreaterThan",
        "triggerThreshold": 0
      },
      "type": "Microsoft.OperationalInsights/workspaces/providers/alertRules"
    }
  ]
}
