{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "String"
    }
  },
  "resources": [
    {
      "apiVersion": "2023-02-01-preview",
      "id": "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/alertRules/b655a06a-31c0-477a-95c2-3726b83d649d')]",
      "kind": "Scheduled",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/b655a06a-31c0-477a-95c2-3726b83d649d')]",
      "properties": {
        "alertRuleTemplateName": "b655a06a-31c0-477a-95c2-3726b83d649d",
        "author": "Florian Roth (Nextron Systems), Samir Bousseaden (idea)",
        "customDetails": null,
        "date": "2019/06/17",
        "description": "Detects a suspicious child process of userinit",
        "displayName": "Suspicious Userinit Child Process",
        "enabled": true,
        "entityMappings": null,
        "eventGroupingSettings": {
          "aggregationKind": "SingleAlert"
        },
        "logsource": {
          "category": "process_creation",
          "product": "windows"
        },
        "modified": "2022/12/09",
        "query": "DeviceProcessEvents\n| where InitiatingProcessFolderPath endswith \"\\\\userinit.exe\" and (not((ProcessCommandLine contains \"\\\\netlogon\\\\\" or (FolderPath endswith \"\\\\explorer.exe\" or ProcessVersionInfoOriginalFileName =~ \"explorer.exe\"))))",
        "queryFrequency": "P1D",
        "queryPeriod": "P1D",
        "sentinelEntitiesMappings": null,
        "severity": "medium",
        "status": "test",
        "suppressionDuration": "PT5H",
        "suppressionEnabled": false,
        "tactics": null,
        "techniques": [
          "T1055"
        ],
        "templateVersion": "1.0.0",
        "triggerOperator": "GreaterThan",
        "triggerThreshold": 0
      },
      "type": "Microsoft.OperationalInsights/workspaces/providers/alertRules"
    }
  ]
}
