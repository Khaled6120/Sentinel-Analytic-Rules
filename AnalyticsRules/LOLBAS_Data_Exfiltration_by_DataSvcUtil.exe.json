{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "String"
    }
  },
  "resources": [
    {
      "apiVersion": "2023-02-01-preview",
      "id": "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/alertRules/e290b10b-1023-4452-a4a9-eb31a9013b3a')]",
      "kind": "Scheduled",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/e290b10b-1023-4452-a4a9-eb31a9013b3a')]",
      "properties": {
        "alertRuleTemplateName": "e290b10b-1023-4452-a4a9-eb31a9013b3a",
        "author": "Ialle Teixeira @teixeira0xfffff, Austin Songer @austinsonger",
        "customDetails": null,
        "date": "2021/09/30",
        "description": "Detects when a user performs data exfiltration by using DataSvcUtil.exe",
        "displayName": "LOLBAS Data Exfiltration by DataSvcUtil.exe",
        "enabled": true,
        "entityMappings": null,
        "eventGroupingSettings": {
          "aggregationKind": "SingleAlert"
        },
        "logsource": {
          "category": "process_creation",
          "product": "windows"
        },
        "modified": "2022/05/16",
        "query": "DeviceProcessEvents\n| where (ProcessCommandLine contains \"/in:\" or ProcessCommandLine contains \"/out:\" or ProcessCommandLine contains \"/uri:\") and (FolderPath endswith \"\\\\DataSvcUtil.exe\" or ProcessVersionInfoOriginalFileName =~ \"DataSvcUtil.exe\")",
        "queryFrequency": "P1D",
        "queryPeriod": "P1D",
        "sentinelEntitiesMappings": null,
        "severity": "medium",
        "status": "test",
        "suppressionDuration": "PT5H",
        "suppressionEnabled": false,
        "tactics": [
          "exfiltration"
        ],
        "techniques": [
          "T1567"
        ],
        "templateVersion": "1.0.0",
        "triggerOperator": "GreaterThan",
        "triggerThreshold": 0
      },
      "type": "Microsoft.OperationalInsights/workspaces/providers/alertRules"
    }
  ]
}
