{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "String"
    }
  },
  "resources": [
    {
      "apiVersion": "2023-02-01-preview",
      "id": "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/alertRules/f0cdd048-82dc-4f7a-8a7a-b87a52b6d0fd')]",
      "kind": "NRT",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/f0cdd048-82dc-4f7a-8a7a-b87a52b6d0fd')]",
      "properties": {
        "alertRuleTemplateName": "f0cdd048-82dc-4f7a-8a7a-b87a52b6d0fd",
        "author": "juju4",
        "customDetails": null,
        "date": "2018/12/11",
        "description": "Detects potential commandline obfuscation using known escape characters",
        "displayName": "Potential Commandline Obfuscation Using Escape Characters",
        "enabled": true,
        "entityMappings": null,
        "eventGroupingSettings": {
          "aggregationKind": "SingleAlert"
        },
        "logsource": {
          "category": "process_creation",
          "product": "windows"
        },
        "modified": "2023/03/03",
        "query": "DeviceProcessEvents\n| where ProcessCommandLine contains \"h^t^t^p\" or ProcessCommandLine contains \"h\\\"t\\\"t\\\"p\"",
        "queryFrequency": "P1D",
        "queryPeriod": "P1D",
        "sentinelEntitiesMappings": null,
        "severity": "medium",
        "status": "test",
        "suppressionDuration": "PT5H",
        "suppressionEnabled": false,
        "tactics": null,
        "techniques": [
          "t1140"
        ],
        "templateVersion": "1.0.0",
        "triggerOperator": "GreaterThan",
        "triggerThreshold": 0
      },
      "type": "Microsoft.OperationalInsights/workspaces/providers/alertRules"
    }
  ]
}
