{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "String"
    }
  },
  "resources": [
    {
      "apiVersion": "2023-02-01-preview",
      "id": "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/alertRules/02fb90de-c321-4e63-a6b9-25f4b03dfd14')]",
      "kind": "Scheduled",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/02fb90de-c321-4e63-a6b9-25f4b03dfd14')]",
      "properties": {
        "alertRuleTemplateName": "02fb90de-c321-4e63-a6b9-25f4b03dfd14",
        "author": "FPT.EagleEye, Thomas Patzke (improvements)",
        "customDetails": null,
        "date": "2021/06/29",
        "description": "Detect DLL Load from Spooler Service backup folder",
        "displayName": "Windows Spooler Service Suspicious Binary Load",
        "enabled": true,
        "entityMappings": null,
        "eventGroupingSettings": {
          "aggregationKind": "SingleAlert"
        },
        "logsource": {
          "category": "image_load",
          "product": "windows"
        },
        "modified": "2022/06/02",
        "query": "DeviceImageLoadEvents\n| where (FolderPath contains \"\\\\Windows\\\\System32\\\\spool\\\\drivers\\\\x64\\\\3\\\\\" or FolderPath contains \"\\\\Windows\\\\System32\\\\spool\\\\drivers\\\\x64\\\\4\\\\\") and FolderPath endswith \".dll\" and InitiatingProcessFolderPath endswith \"\\\\spoolsv.exe\"",
        "queryFrequency": "P1D",
        "queryPeriod": "P1D",
        "sentinelEntitiesMappings": null,
        "severity": "informational",
        "status": "test",
        "suppressionDuration": "PT5H",
        "suppressionEnabled": false,
        "tactics": [
          "Persistence"
        ],
        "techniques": [
          "T1574"
        ],
        "templateVersion": "1.0.0",
        "triggerOperator": "GreaterThan",
        "triggerThreshold": 0
      },
      "type": "Microsoft.OperationalInsights/workspaces/providers/alertRules"
    }
  ]
}
