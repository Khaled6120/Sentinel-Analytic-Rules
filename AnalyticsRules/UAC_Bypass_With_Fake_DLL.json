{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "String"
    }
  },
  "resources": [
    {
      "apiVersion": "2023-02-01-preview",
      "id": "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/alertRules/a5ea83a7-05a5-44c1-be2e-addccbbd8c03')]",
      "kind": "NRT",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/a5ea83a7-05a5-44c1-be2e-addccbbd8c03')]",
      "properties": {
        "alertRuleTemplateName": "a5ea83a7-05a5-44c1-be2e-addccbbd8c03",
        "author": "oscd.community, Dmitry Uchakin",
        "customDetails": null,
        "date": "2020/10/06",
        "description": "Attempts to load dismcore.dll after dropping it",
        "displayName": "UAC Bypass With Fake DLL",
        "enabled": true,
        "entityMappings": null,
        "eventGroupingSettings": {
          "aggregationKind": "SingleAlert"
        },
        "logsource": {
          "category": "image_load",
          "product": "windows"
        },
        "modified": "2022/12/25",
        "query": "DeviceImageLoadEvents\n| where (FolderPath endswith \"\\\\dismcore.dll\" and InitiatingProcessFolderPath endswith \"\\\\dism.exe\") and (not(FolderPath =~ \"C:\\\\Windows\\\\System32\\\\Dism\\\\dismcore.dll\"))",
        "queryFrequency": "P1D",
        "queryPeriod": "P1D",
        "sentinelEntitiesMappings": null,
        "severity": "high",
        "status": "test",
        "suppressionDuration": "PT5H",
        "suppressionEnabled": false,
        "tactics": [
          "persistence"
        ],
        "techniques": [
          "t1548",
          "t1574"
        ],
        "templateVersion": "1.0.0",
        "triggerOperator": "GreaterThan",
        "triggerThreshold": 0
      },
      "type": "Microsoft.OperationalInsights/workspaces/providers/alertRules"
    }
  ]
}
