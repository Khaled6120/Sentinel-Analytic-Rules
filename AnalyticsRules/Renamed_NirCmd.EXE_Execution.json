{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "String"
    }
  },
  "resources": [
    {
      "apiVersion": "2023-02-01-preview",
      "id": "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/alertRules/264982dc-dbad-4dce-b707-1e0d3e0f73d9')]",
      "kind": "Scheduled",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/264982dc-dbad-4dce-b707-1e0d3e0f73d9')]",
      "properties": {
        "alertRuleTemplateName": "264982dc-dbad-4dce-b707-1e0d3e0f73d9",
        "author": "X__Junior (Nextron Systems)",
        "customDetails": null,
        "date": "2024/03/11",
        "description": "Detects the execution of a renamed \"NirCmd.exe\" binary based on the PE metadata fields.",
        "displayName": "Renamed NirCmd.EXE Execution",
        "enabled": true,
        "entityMappings": null,
        "eventGroupingSettings": {
          "aggregationKind": "SingleAlert"
        },
        "logsource": {
          "category": "process_creation",
          "product": "windows"
        },
        "modified": "",
        "query": "DeviceProcessEvents\n| where ProcessVersionInfoOriginalFileName =~ \"NirCmd.exe\" and (not((FolderPath endswith \"\\\\nircmd.exe\" or FolderPath endswith \"\\\\nircmdc.exe\")))",
        "queryFrequency": "P1D",
        "queryPeriod": "P1D",
        "sentinelEntitiesMappings": null,
        "severity": "high",
        "status": "experimental",
        "suppressionDuration": "PT5H",
        "suppressionEnabled": false,
        "tactics": [
          "execution"
        ],
        "techniques": [
          "T1059",
          "T1202"
        ],
        "templateVersion": "1.0.0",
        "triggerOperator": "GreaterThan",
        "triggerThreshold": 0
      },
      "type": "Microsoft.OperationalInsights/workspaces/providers/alertRules"
    }
  ]
}
