{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "String"
    }
  },
  "resources": [
    {
      "apiVersion": "2023-02-01-preview",
      "id": "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/alertRules/8a4519e8-e64a-40b6-ae85-ba8ad2177559')]",
      "kind": "Scheduled",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/8a4519e8-e64a-40b6-ae85-ba8ad2177559')]",
      "properties": {
        "alertRuleTemplateName": "8a4519e8-e64a-40b6-ae85-ba8ad2177559",
        "author": "Max Altgelt (Nextron Systems)",
        "customDetails": null,
        "date": "2022/06/02",
        "description": "Detects process creation with a renamed BrowserCore.exe (used to extract Azure tokens)",
        "displayName": "Renamed BrowserCore.EXE Execution",
        "enabled": true,
        "entityMappings": null,
        "eventGroupingSettings": {
          "aggregationKind": "SingleAlert"
        },
        "logsource": {
          "category": "process_creation",
          "product": "windows"
        },
        "modified": "2023/02/03",
        "query": "DeviceProcessEvents\n| where ProcessVersionInfoOriginalFileName =~ \"BrowserCore.exe\" and (not(FolderPath endswith \"\\\\BrowserCore.exe\"))",
        "queryFrequency": "P1D",
        "queryPeriod": "P1D",
        "sentinelEntitiesMappings": null,
        "severity": "high",
        "status": "test",
        "suppressionDuration": "PT5H",
        "suppressionEnabled": false,
        "tactics": [],
        "techniques": [
          "T1036",
          "T1528"
        ],
        "templateVersion": "1.0.0",
        "triggerOperator": "GreaterThan",
        "triggerThreshold": 0
      },
      "type": "Microsoft.OperationalInsights/workspaces/providers/alertRules"
    }
  ]
}
