{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "String"
    }
  },
  "resources": [
    {
      "apiVersion": "2023-02-01-preview",
      "id": "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/alertRules/883835a7-df45-43e4-bf1d-4268768afda4')]",
      "kind": "NRT",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/883835a7-df45-43e4-bf1d-4268768afda4')]",
      "properties": {
        "alertRuleTemplateName": "883835a7-df45-43e4-bf1d-4268768afda4",
        "author": "Florian Roth (Nextron Systems)",
        "customDetails": null,
        "date": "2021/05/27",
        "description": "Detects a regedit started with TrustedInstaller privileges or by ProcessHacker.exe",
        "displayName": "Regedit as Trusted Installer",
        "enabled": true,
        "entityMappings": null,
        "eventGroupingSettings": {
          "aggregationKind": "SingleAlert"
        },
        "logsource": {
          "category": "process_creation",
          "product": "windows"
        },
        "modified": "2022/10/09",
        "query": "DeviceProcessEvents\n| where FolderPath endswith \"\\\\regedit.exe\" and (InitiatingProcessFolderPath endswith \"\\\\TrustedInstaller.exe\" or InitiatingProcessFolderPath endswith \"\\\\ProcessHacker.exe\")",
        "queryFrequency": "P1D",
        "queryPeriod": "P1D",
        "sentinelEntitiesMappings": null,
        "severity": "high",
        "status": "test",
        "suppressionDuration": "PT5H",
        "suppressionEnabled": false,
        "tactics": null,
        "techniques": [
          "t1548"
        ],
        "templateVersion": "1.0.0",
        "triggerOperator": "GreaterThan",
        "triggerThreshold": 0
      },
      "type": "Microsoft.OperationalInsights/workspaces/providers/alertRules"
    }
  ]
}
