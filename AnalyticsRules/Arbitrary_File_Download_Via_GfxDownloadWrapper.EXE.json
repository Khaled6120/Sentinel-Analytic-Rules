{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "String"
    }
  },
  "resources": [
    {
      "apiVersion": "2023-02-01-preview",
      "id": "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/alertRules/eee00933-a761-4cd0-be70-c42fe91731e7')]",
      "kind": "NRT",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/eee00933-a761-4cd0-be70-c42fe91731e7')]",
      "properties": {
        "alertRuleTemplateName": "eee00933-a761-4cd0-be70-c42fe91731e7",
        "author": "Victor Sergeev, oscd.community",
        "customDetails": null,
        "date": "2020/10/09",
        "description": "Detects execution of GfxDownloadWrapper.exe with a URL as an argument to download file.",
        "displayName": "Arbitrary File Download Via GfxDownloadWrapper.EXE",
        "enabled": true,
        "entityMappings": null,
        "eventGroupingSettings": {
          "aggregationKind": "SingleAlert"
        },
        "logsource": {
          "category": "process_creation",
          "product": "windows"
        },
        "modified": "2023/10/18",
        "query": "DeviceProcessEvents\n| where ((ProcessCommandLine contains \"http://\" or ProcessCommandLine contains \"https://\") and FolderPath endswith \"\\\\GfxDownloadWrapper.exe\") and (not(ProcessCommandLine contains \"https://gameplayapi.intel.com/\"))",
        "queryFrequency": "P1D",
        "queryPeriod": "P1D",
        "sentinelEntitiesMappings": null,
        "severity": "medium",
        "status": "test",
        "suppressionDuration": "PT5H",
        "suppressionEnabled": false,
        "tactics": null,
        "techniques": [
          "t1105"
        ],
        "templateVersion": "1.0.0",
        "triggerOperator": "GreaterThan",
        "triggerThreshold": 0
      },
      "type": "Microsoft.OperationalInsights/workspaces/providers/alertRules"
    }
  ]
}
