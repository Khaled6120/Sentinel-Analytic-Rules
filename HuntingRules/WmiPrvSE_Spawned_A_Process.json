{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "String"
    }
  },
  "resources": [
    {
      "type": "Microsoft.OperationalInsights/workspaces/savedSearches",
      "apiVersion": "2020-08-01",
      "name": "[concat(parameters('workspace'), '/WmiPrvSE_Spawned_A_Process')]",
      "location": "[resourceGroup().location]",
      "properties": {
        "eTag": "*",
        "displayName": "WmiPrvSE Spawned A Process",
        "category": "Hunting Queries",
        "query": "DeviceProcessEvents\n| where InitiatingProcessFolderPath endswith \"\\\\WmiPrvSe.exe\" and (not(((LogonId in~ (\"0x3e7\", \"null\")) or isnull(LogonId) or (AccountName contains \"AUTHORI\" or AccountName contains \"AUTORI\") or FolderPath endswith \"\\\\WerFault.exe\" or FolderPath endswith \"\\\\WmiPrvSE.exe\")))",
        "version": 1,
        "tags": [
          {
            "name": "description",
            "value": "Detects WmiPrvSE spawning a proc"
          },
          {
            "name": "tactics",
            "value": "execution"
          },
          {
            "name": "relevantTechniques",
            "value": "T1047"
          }
        ]
      }
    }
  ]
}
